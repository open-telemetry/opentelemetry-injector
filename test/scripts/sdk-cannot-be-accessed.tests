# DOTNET_STARTUP_HOOKS
run_test_case \
  "does not add DOTNET_STARTUP_HOOKS if it the .NET auto instrumentation is present but cannot be accessed" \
  "app" \
  "node index.js dotnet-startup-hooks" \
  "DOTNET_STARTUP_HOOKS: -"

# JAVA_TOOL_OPTIONS
run_test_case \
  "does not add JAVA_TOOL_OPTIONS if it the Java auto instrumentation is present but cannot be accessed" \
  "app" \
  "node index.js java-tool-options" \
  "JAVA_TOOL_OPTIONS: -"

# NODE_OPTIONS
run_test_case \
  "does not add NODE_OPTIONS if it the Node.js auto instrumentation is present but cannot be accessed" \
  "app" \
  "node index.js node-options" \
  "NODE_OPTIONS: -"
